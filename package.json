{
  "name": "kicker-league",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=development babel src -s -D -d dist",
    "prestart": "npm run -s build",
    "start": "node dist/server.js",
    "start:dev": "nodemon -w src --exec babel-node src/server.js",
    "db:migrate": "sequelize db:migrate",
    "db:dev:clean": "rm -f db.development.sqlite",
    "db:dev:seeds": "sequelize db:seed:all",
    "db:dev:rebuild": "npm run db:dev:clean && npm run db:migrate && npm run db:dev:seeds",
    "lint": "eslint config src migrations seeders",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test mocha test/integration/*.test.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.4",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.0.8",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^3.1.3",
    "eslint": "^4.19.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.17.2",
    "pre-commit": "^1.2.2",
    "supertest": "^2.0.0"
  }
}

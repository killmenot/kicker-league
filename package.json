{
  "name": "kicker-league",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=development babel src -s -D -d dist",
    "prestart": "npm run -s build",
    "start": "node index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon -w src --exec babel-node src/server.js",
    "db:migrate": "sequelize db:migrate",
    "db:dev:clean": "rm -f db.development.sqlite",
    "db:dev:seeds": "sequelize db:seed:all",
    "db:dev:rebuild": "npm run db:dev:clean && npm run db:migrate && npm run db:dev:seeds",
    "lint": "eslint config src migrations seeders",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test mocha test/integration/*.test.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "pg": "^8.6.0",
    "pug": "^3.0.2",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.2.0",
    "serve-favicon": "~2.5.0",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "expect.js": "^0.3.1",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "pre-commit": "^1.2.2",
    "supertest": "^6.1.3"
  }
}
